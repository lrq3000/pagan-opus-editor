<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item
        android:state_pressed="true"
        app:state_focused="true"
        android:drawable="@drawable/label_focused_pressed" />
    <item
        android:state_pressed="false"
        app:state_focused="true"
        android:drawable="@drawable/label_focused" />
    <item
        android:state_pressed="true"
        app:state_focused="false"
        android:drawable="@drawable/label_pressed" />
    <item
        android:state_pressed="false"
        app:state_focused="false"
        android:drawable="@drawable/label_default" />
</selector>
