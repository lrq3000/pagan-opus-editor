<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
<item
    android:elevation="0dp"
    android:state_pressed="true"
    app:state_active="false"
    android:color="?attr/numberselector_fg_pressed"/>
<item
    android:state_pressed="false"
    app:state_active="false"
    android:color="?attr/numberselector_fg_default"/>
<item
    android:state_pressed="false"
    app:state_active="true"
    android:color="?attr/numberselector_fg_selected"/>
<item
    android:elevation="0dp"
    android:state_pressed="true"
    app:state_active="true"
    android:color="?attr/numberselector_fg_selected_pressed"/>
</selector>
